{
  "name": "expo-ble-boilerplate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build-local-android": "eas build --profile development --platform android --local",
    "build-android-distribution": "eas build --profile production --platform android --local --output ./playstore_build.aab",
    "build-android-apks": "./scripts/build_android.sh",
    "install-android-apks": "bundletool install-apks --apks=./playstore_build.apks",
    "build-ios-distribution": "eas build --profile production --platform ios --local --non-interactive",
    "build-ios-simulator": "eas build --platform ios --profile simulator --local --non-interactive",
    "checkPath": "if [[ $(pwd) =~ .\\ . ]]; then echo 'Error: Path contains space.' && exit 1; else echo 'Path check passed.'; fi",
    "prestart": "npm run checkPath",
    "preios": "npm run checkPath",
    "prebuild-ios-distribution": "npm run checkPath",
    "preclean-ibuild-all": "npm run checkPath"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.0.2",
    "expo": "~51.0.14",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.16",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.6",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.2",
    "react-native-ble-plx": "^3.2.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10",
    "expo-dev-client": "~4.0.18"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.1",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
