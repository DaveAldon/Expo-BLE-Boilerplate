{
  "cli": {
    "version": ">= 8.0.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "withoutCredentials": false,
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    },
    "simulator": {
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      }
    },
    "production": {
      "releaseChannel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
